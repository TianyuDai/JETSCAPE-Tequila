<jetscape>
  
  <nEvents> 10000 </nEvents>
  
  <outputFilename updated="yes">test_out</outputFilename>
  <JetScapeWriterAscii> on </JetScapeWriterAscii>

  <Random>
    <seed>0</seed>
  </Random>

  
  <Hard>
    <PythiaGun>
      <pTHatMin updated="yes">5.0</pTHatMin>
      <pTHatMax updated="yes">10.0</pTHatMax>
      <eCM>200</eCM>
      <FSR_on>0</FSR_on>
    </PythiaGun>
  </Hard>
  
  <Eloss>

    <Matter>
      <in_vac> 1 </in_vac>
      <Q0> 1.0 </Q0>
      <vir_factor> 0.25 </vir_factor>
      <recoil_on> 0 </recoil_on>
      <broadening_on> 0 </broadening_on>
      <brick_med> 0 </brick_med>
    </Matter>

  </Eloss>

  <JetHadronization>
    <name>colorless</name>
  </JetHadronization>
  
</jetscape>

<?xml version="1.0"?>

<jetscape>
  
  <nEvents> 30000 </nEvents>
  
  <JetScapeWriterAscii> on </JetScapeWriterAscii>
  <outputFilename>test_out</outputFilename>
  
  <!-- Hard Process -->
  <Hard>
    <PythiaGun>
      <pTHatMin>1000</pTHatMin>
      <pTHatMax>1380</pTHatMax>
      <eCM>2760</eCM>
    </PythiaGun>
  </Hard>
  
  <!--Eloss Modules -->
  <Eloss>
    <Matter> 
      <Q0> 2.0 </Q0>
      <in_vac> 1 </in_vac>
      <vir_factor> 0.25 </vir_factor>
      <recoil_on> 0 </recoil_on>
      <broadening_on> 0 </broadening_on>
      <brick_med> 0 </brick_med>
    </Matter>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
    <name>colorless</name>
    <take_recoil>1</take_recoil>
    <eCMforHadronization>1380</eCMforHadronization>
  </JetHadronization>
  
</jetscape>
